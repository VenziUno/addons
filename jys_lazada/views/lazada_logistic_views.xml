<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_lazada_logistic_tree" model="ir.ui.view">
            <field name="name">lazada.logistic.tree</field>
            <field name="model">lazada.logistic</field>
            <field name="arch" type="xml">
                <tree string="Lazada Logistics" create="0">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_lazada_logistic_form" model="ir.ui.view">
            <field name="name">lazada.logistic.form</field>
            <field name="model">lazada.logistic</field>
            <field name="arch" type="xml">
                <form string="Lazada Logistics" create="0" edit="0">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name" required="1"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_jys_lazada_logistic" model="ir.actions.act_window">
            <field name="name">Logistics</field>
            <field name="res_model">lazada.logistic</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

    </data>
</odoo>