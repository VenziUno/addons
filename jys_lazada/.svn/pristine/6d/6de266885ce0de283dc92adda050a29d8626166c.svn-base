<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_lazada_warehouse_tree" model="ir.ui.view">
            <field name="name">lazada.warehouse.tree</field>
            <field name="model">lazada.warehouse</field>
            <field name="arch" type="xml">
                <tree string="Lazada Warehouses" create="0">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record id="view_lazada_warehouse_form" model="ir.ui.view">
            <field name="name">lazada.warehouse.form</field>
            <field name="model">lazada.warehouse</field>
            <field name="arch" type="xml">
                <form string="Lazada Warehouses" create="0" edit="0">
                    <sheet>
                        <div class="oe_title">
                            <label for="name" class="oe_edit_only"/>
                            <h1>
                                <field name="name" required="1"/>
                                <field name="code" required="1"/>
                            </h1>
                        </div>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_jys_lazada_warehouse" model="ir.actions.act_window">
            <field name="name">Warehouses</field>
            <field name="res_model">lazada.warehouse</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{}</field>
        </record>

    </data>
</odoo>