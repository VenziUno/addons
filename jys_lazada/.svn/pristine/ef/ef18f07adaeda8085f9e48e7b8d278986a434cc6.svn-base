<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_stock_picking_lazada_label" model="ir.ui.view">
            <field name="name">view.stock.picking.lazada.label</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.vpicktree"/>
            <field name="arch" type="xml">
                <xpath expr="//header" position="inside">
                    <button name="print_label_lz" type="object" class="oe_highlight" string="Lazada Picking Slip"/>
                </xpath>
                <xpath expr="//header/button[@name='do_unreserve']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//header/button[@name='action_assign']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>