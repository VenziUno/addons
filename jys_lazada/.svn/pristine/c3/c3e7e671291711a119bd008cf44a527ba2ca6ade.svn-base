<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_lazada_get_product_form" model="ir.ui.view">
            <field name="name">Lazada Get Product</field>
            <field name="model">lazada.get.product</field>
            <field name="arch" type="xml">
                <form string="Lazada Get Product">
                    <p>
                        Integration Lazada to System.
                    </p>
                    <separator string="Shops"/>
                    <field name="shop_id" nolabel="1" required="1"/>
                    <footer>
                        <button name="create_product" string="Confirm" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_jys_lazada_get_product" model="ir.actions.act_window">
            <field name="name">Get Product</field>
            <field name="res_model">lazada.get.product</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_lazada_get_product_form"/>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
