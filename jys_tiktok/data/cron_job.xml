<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1"> 
        <record forcecreate="True" id="ir_cron_tiktok_get_order" model="ir.cron">
            <field name="name">Get TikTok Orders</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model.auto_tiktok_get_orders()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>

        <record forcecreate="True" id="ir_cron_tiktok_access_token" model="ir.cron">
            <field name="name">Get Access Token</field>
            <field name="model_id" ref="model_product_template"/>
            <field name="state">code</field>
            <field name="code">model.auto_action_get_access_token_tiktok()</field>
            <field name="active" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">5</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>

</odoo>